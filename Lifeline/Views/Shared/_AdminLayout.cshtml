                                    <a href="#" data-action="click-trigger" class="menunav">
                                        <span class="menu-icon"><i class="fa fa-sitemap"></i></span>
                                        <span class="menu-text">Locations</span>
                                        <span class="menu-badge"><span class="badge vd_bg-black-30"><i class="fa fa-angle-down"></i></span></span>
                                    </a>
                                    <div class="child-menu divmenunav " data-action="click-target" id="limenucoordinators">
                                        <ul>
                                            <li class="">
                                                <a href="@Url.Action("Locations", "Coordinators")" id="lnkcoordinatorsLocations">
